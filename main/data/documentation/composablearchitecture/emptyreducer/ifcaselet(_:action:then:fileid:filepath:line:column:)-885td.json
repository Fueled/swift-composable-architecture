ifier"},{"kind":"text","text":"> "},{"text":"where","kind":"keyword"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"CaseState"},{"kind":"text","text":" == "},{"kind":"typeIdentifier","text":"Case"},{"text":".","kind":"text"},{"preciseIdentifier":"s:22ComposableArchitecture7ReducerP5StateQa","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/State","kind":"typeIdentifier","text":"State"},{"text":", ","kind":"text"},{"text":"CaseAction","kind":"typeIdentifier"},{"kind":"text","text":" == "},{"text":"Case","kind":"typeIdentifier"},{"kind":"text","text":"."},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action","kind":"typeIdentifier","preciseIdentifier":"s:22ComposableArchitecture7ReducerP6ActionQa","text":"Action"},{"kind":"text","text":", "},{"text":"Case","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","kind":"typeIdentifier","text":"Reducer"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Self"},{"text":".","kind":"text"},{"text":"Action","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action","preciseIdentifier":"s:22ComposableArchitecture7ReducerP6ActionQa","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:9CasePaths0A8PathableP","kind":"typeIdentifier","text":"CasePathable"},{"text":", ","kind":"text"},{"text":"Self","kind":"typeIdentifier"},{"text":".","kind":"text"},{"preciseIdentifier":"s:22ComposableArchitecture7ReducerP5StateQa","kind":"typeIdentifier","text":"State","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/State"},{"kind":"text","text":" : "},{"text":"CasePathable","kind":"typeIdentifier","preciseIdentifier":"s:9CasePaths0A8PathableP"}]}]},{"kind":"parameters","parameters":[{"name":"toCaseState","content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent state to a case containing child state.","type":"text"}]}]},{"name":"toCaseAction","content":[{"inlineContent":[{"type":"text","text":"A case path from parent action to a case containing child actions."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against child state when it is"},{"text":" ","type":"text"},{"type":"text","text":"present"}],"type":"paragraph"}],"name":"case"}]},{"kind":"content","content":[{"level":2,"anchor":"return-value","type":"heading","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"inlineContent":[{"text":"For example, if a parent feature’s state is expressed as an enum of multiple children","type":"text"},{"text":" ","type":"text"},{"text":"states, then ","type":"text"},{"type":"codeVoice","code":"ifCaseLet"},{"text":" can run a child reducer on a particular case of the enum:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["@Reducer","struct Parent {","  enum State {","    case loggedIn(Authenticated.State)","    case loggedOut(Unauthenticated.State)","  }","  enum Action {","    case loggedIn(Authenticated.Action)","    case loggedOut(Unauthenticated.Action)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifCaseLet(\\.loggedIn, action: \\.loggedIn) {","      Authenticated()","    }","    .ifCaseLet(\\.loggedOut, action: \\.loggedOut) {","      Unauthenticated()","    }","  }","}"]},{"inlineContent":[{"text":"The ","type":"text"},{"type":"codeVoice","code":"ifCaseLet"},{"type":"text","text":" operator does a number of things to try to enforce correctness:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It forces a specific order of operations for the child and parent features. It runs the"},{"text":" ","type":"text"},{"type":"text","text":"child first, and then the parent. If the order was reversed, then it would be possible for"},{"type":"text","text":" "},{"type":"text","text":"the parent feature to change the case of the child enum, in which case the child"},{"type":"text","text":" "},{"type":"text","text":"feature would not be able to react to that action. That can cause subtle bugs."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It automatically cancels all child effects when it detects the child enum case changes."}]}]}]},{"inlineContent":[{"type":"text","text":"It is still possible for a parent feature higher up in the application to change the case of"},{"type":"text","text":" "},{"text":"the enum before the child has a chance to react to the action. In such cases a runtime","type":"text"},{"type":"text","text":" "},{"type":"text","text":"warning is shown in Xcode to let you know that there’s a potential problem."}],"type":"paragraph"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/composablearchitecture\/emptyreducer\/ifcaselet(_:action:then:fileid:filepath:line:column:)-885td"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/EmptyReducer\/ifCaseLet(_:action:then:fileID:filePath:line:column:)-885td","interfaceLanguage":"swift"},"abstract":[{"text":"Embeds a child reducer in a parent domain that works on a case of parent enum state.","type":"text"}],"schemaVersion":{"major":0,"patch":0,"minor":3},"sections":[],"metadata":{"symbolKind":"method","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"ifCaseLet","kind":"identifier"},{"k